{
  "name": "react-str",
  "description": "React data fetching hooks using stale-till-revalidate policy.",
  "version": "0.0.3",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "license": "MIT",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "add-assets": "copyfiles -u 1 'src/**/!(*.js|*.jsx|*.map|*.ts|*.tsx)' lib/",
    "build": "nwb build-react-component",
    "build:ts": "tsc",
    "build:all": "yarn clean && yarn build:ts && yarn add-assets",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "yarn build:all"
  },
  "author": "tejasupmanyu",
  "dependencies": {
    "object-hash": "^2.0.3"
  },
  "peerDependencies": {
    "react": "^16.8.x"
  },
  "devDependencies": {
    "@types/object-hash": "^1.3.1",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "awesome-typescript-loader": "^5.2.1",
    "copyfiles": "^2.1.1",
    "nwb": "0.23.x",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.6.3"
  },
  "keywords": [
    "react",
    "data-fetching",
    "str"
  ]
}
